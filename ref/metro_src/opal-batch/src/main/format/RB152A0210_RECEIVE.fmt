 #-------------------------------------------------------------------------------
 # プッシュ通知指示結果応答電文レイアウト定義
 #-------------------------------------------------------------------------------
 file-type:        "JSON"
 text-encoding:    "UTF-8"

 [data]
 1  status                            X
 2  result[0..1]                      OB
 3  error[0..1]                       OB

 [result]
 1  id[0..1]                          X

 [error]
 1  code[0..1]                        X
 2  message[0..1]                     X