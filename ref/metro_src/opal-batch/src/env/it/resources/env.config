# JDBC接続ドライバクラス(DataSourceを直接使用する際の項目)
nablarch.db.jdbcDriver=oracle.jdbc.driver.OracleDriver

# JDBC接続URL(DataSourceを直接使用する際の項目)
nablarch.db.url=jdbc:oracle:thin:@localhost:1521/opal

# DB接続ユーザ名(DataSourceを直接使用する際の項目)
nablarch.db.user=opal

# DB接続パスワード(DataSourceを直接使用する際の項目)
nablarch.db.password=opal

# 最大プールサイズ(DataSourceを直接使用する際の項目)
nablarch.db.maxPoolSize=10

# 初期プールサイズ(DataSourceを直接使用する際の項目)
nablarch.db.initialPoolSize=2

# フォーマット定義ファイルの格納ディレクトリ
nablarch.filePathSetting.basePathSettings.format=file:./src/main/format

# ファイル伝送用ディレクトリ（アプリ会員管理⇒基幹）
nablarch.filePathSetting.basePathSettings.output=file:./work/transfer_temp

# ファイル伝送用ディレクトリ（基幹⇒アプリ会員管理）
nablarch.filePathSetting.basePathSettings.input=file:./work/transfer_temp

# 伝送ファイル管理ディレクトリ（アプリ会員管理⇒クルメル）
nablarch.filePathSetting.basePathSettings.mail=file:./work/transfer_temp

# 伝送ファイル管理ディレクトリ（クルメル⇒アプリ会員管理）
nablarch.filePathSetting.basePathSettings.mailIn=file:./work/transfer_temp

# エラーファイルの出力先ディレクトリ
nablarch.filePathSetting.basePathSettings.error=file:./work/transfer_temp

#クルメル接続用パスワード
curumeru_tansport_password=123

# リトライ回数
nablarch.retryHandler.retryCount=3

# リトライ間隔
nablarch.retryHandler.retryIntervals=5000

# データの監視間隔設定
nablarch.fw.handler.ProcessResidentHandler.dataWatchInterval=50000

#メール配信一括指示_マイル移行_出力上限件数
mail_del_lump_instr_mile_trans_max_count=3

#メール配信一括指示_マイル調整_出力上限件数
mail_del_lump_instr_mile_adjust_max_count=3

#メール配信一時的エラー発生回数上限
mail_temporary_error_max_availe_count=2

#取得件数_メール配信個別指示
lite_mail_inv_count=12

#メール配信個別指示_通信に使用するHTTPメソッド
opalCurumeruHttpClient.RB151A0110.httpMethod=POST
#メール配信個別指示_接続先uri
opalCurumeruHttpClient.RB151A0110.uri=http://localhost:8080/opal-httpmessaging-test/MockMailLiteDeliver
#メール配信個別指示_コネクションタイムアウト(単位:ミリ秒)
opalCurumeruHttpClient.RB151A0110.httpConnectTimeout=10000
#メール配信個別指示_読み取りタイムアウト(単位:ミリ秒)
opalCurumeruHttpClient.RB151A0110.httpReadTimeout=10000
#メール配信個別指示_要求電文をログに出力するかどうか
opalCurumeruHttpClient.RB151A0110.isOutputRequestMessage=true
#メール配信個別指示_応答電文をログに出力するかどうか
opalCurumeruHttpClient.RB151A0110.isOutputReceivedMessage=true

#メール配信一括指示_通信に使用するHTTPメソッド
opalCurumeruHttpClient.RB151A0210.httpMethod=POST
#メール配信一括指示_接続先uri
opalCurumeruHttpClient.RB151A0210.uri=http://localhost:8080/opal-httpmessaging-test/MockMailPackDeliver
#メール配信一括指示_コネクションタイムアウト(単位:ミリ秒)
opalCurumeruHttpClient.RB151A0210.httpConnectTimeout=10000
#メール配信一括指示_読み取りタイムアウト(単位:ミリ秒)
opalCurumeruHttpClient.RB151A0210.httpReadTimeout=10000
#メール配信一括指示_要求電文をログに出力するかどうか
opalCurumeruHttpClient.RB151A0210.isOutputRequestMessage=true
#メール配信一括指示_応答電文をログに出力するかどうか
opalCurumeruHttpClient.RB151A0210.isOutputReceivedMessage=true

#プッシュ通知一括指示_遅延時間(分)
push_notice_instr_delaytime=5
#プッシュ通知一括指示_タイムアウト上限回数
push_notice_instr_timeout_limit_count=2

#プッシュ通知一括指示_popinfo接続時にHTTPヘッダーに設定する認証トークンの設定
push_notice_instr_popinfo_auth=00000000000000000000000000000000

#プッシュ通知一括指示_通信に使用するHTTPメソッド
messageSender.RB152A0210.httpMethod=POST
#プッシュ通知一括指示_通信クライアント(論理名)
messageSender.RB152A0210.messageSenderClient=messageSenderClient
#プッシュ通知一括指示_接続先uri
messageSender.RB152A0210.uri=http://localhost:8080/opal-httpmessaging-test/MockPushNotice
#プッシュ通知一括指示_コネクションタイムアウト(単位:ミリ秒)
messageSender.RB152A0210.httpConnectTimeout=10000
#プッシュ通知一括指示_読み取りタイムアウト(単位:ミリ秒)
messageSender.RB152A0210.httpReadTimeout=10000

#エラーメールアドレス管理_通信に使用するHTTPメソッド
opalCurumeruHttpClient.RB153A0110.httpMethod=POST
#エラーメールアドレス管理_接続先uri
opalCurumeruHttpClient.RB153A0110.uri=http://localhost:8080/opal-httpmessaging-test/MockErrMailControl
#エラーメールアドレス管理_コネクションタイムアウト(単位:ミリ秒)
opalCurumeruHttpClient.RB153A0110.httpConnectTimeout=10000
#エラーメールアドレス管理_読み取りタイムアウト(単位:ミリ秒)
opalCurumeruHttpClient.RB153A0110.httpReadTimeout=10000
#エラーメールアドレス管理_要求電文をログに出力するかどうか
opalCurumeruHttpClient.RB153A0110.isOutputRequestMessage=true
#エラーメールアドレス管理_応答電文をログに出力するかどうか
opalCurumeruHttpClient.RB153A0110.isOutputReceivedMessage=true

